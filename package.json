{
	"name": "zicott",
	"description": "A CLI tool to easily download YouTube music",
	"version": "0.3.2",
	"license": "MIT",
	"author": {
		"name": "yunger",
		"url": "https://yunger.dev"
	},
	"keywords": [
		"cli",
		"youtube",
		"music"
	],
	"homepage": "https://github.com/yunger7/zicott",
	"bugs": {
		"url": "https://github.com/yunger7/zicott/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/yunger7/zicott.git"
	},
	"types": "build/types/types.d.ts",
	"bin": {
		"zicott": "bin/zicott"
	},
	"scripts": {
		"dev": "tsc -w",
		"build": "npm run clean && tsc && cp -r ./src/assets ./build",
		"format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
		"clean": "rm -rf ./build",
		"release": "npm run build && npm publish"
	},
	"files": [
		"build",
		"LICENSE",
		"README.md",
		"bin"
	],
	"dependencies": {
		"cli-progress": "^3.11.2",
		"fluent-ffmpeg": "^2.1.3",
		"gluegun": "latest",
		"ts-node": "^10.9.2",
		"ytdl-core": "^4.11.5",
		"ytpl": "^2.3.0"
	},
	"devDependencies": {
		"@types/cli-progress": "^3.11.0",
		"@types/fluent-ffmpeg": "^2.1.24",
		"@types/node": "^12.7.11",
		"@typescript-eslint/eslint-plugin": "^4.17.0",
		"@typescript-eslint/parser": "^4.17.0",
		"eslint": "^7.22.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-prettier": "^3.3.1",
		"prettier": "^2.2.1",
		"typescript": "^4.2.3"
	}
}
